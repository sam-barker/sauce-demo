{
    "compilerOptions": {
      "outDir": "./.tsbuild/",
      "sourceMap": false,
      "target": "esnext",
      "module": "commonjs",
      "removeComments": true,
      "noImplicitAny": true,
      "strictPropertyInitialization": true,
      "esModuleInterop": true,
      "strictNullChecks": true,
      "types": ["node", "webdriverio/async", "@wdio/mocha-framework", "expect-webdriverio"]
    },
    "include": ["test/**/*.ts", "./src/types/**/*.ts"],
    "exclude": [
      "node_modules"
    ]
}
